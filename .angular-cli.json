{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "lev-livet-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/fullcalendar/dist/fullcalendar.min.css",
        "libraries/bootstrap/css/bootstrap.min.css",
        "libraries/fonts/simple-line-icons.min.css",
        "libraries/css/Collapsible-sidebar-left-or-right--Content-overlay1.css",
        "libraries/css/Navigation-with-Button1.css",
        "libraries/css/SIdebar-Responsive-2.css",
        "libraries/css/SIdebar-Responsive-21.css",
        "libraries/css/styles.css",
        "libraries/css/icon.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "libraries/js/jquery.min.js",
        "libraries/bootstrap/js/bootstrap.min.js",
        "libraries/js/jquery.nicescroll.min.js",
        "libraries/js/Collapsible-sidebar-left-or-right--Content-overlay.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
